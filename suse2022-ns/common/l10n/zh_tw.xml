    <l:template name="intra-preface" text="序言 「%t」"/>
    <l:template name="intra-procedure" text="程序 %n 「%t」"/>
    <l:template name="intra-table" text="表格 %n 「%t」"/>

    <l:template name="intra-variablelist" text="「%t」"/>
