    <l:template name="intra-example" text="例&#160;%n “%t”"/>
    <l:template name="intra-figure" text="图&#160;%n “%t”"/>
    <l:template name="intra-preface" text="前言 “%t”"/>
    <l:template name="intra-procedure" text="过程&#160;%n “%t”"/>
    <l:template name="intra-table" text="表&#160;%n “%t”"/>

    <l:template name="intra-variablelist" text="“%t”"/>
